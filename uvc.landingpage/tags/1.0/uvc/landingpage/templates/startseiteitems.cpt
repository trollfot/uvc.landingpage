<div class="row">
  <div class="span14">
    <div class="row image-row" tal:repeat="items viewletmanager.getTupleItems()">
      <div class="span7" tal:repeat="item items">
        <a href="" tal:attributes="href item['action']">
          <img class="landingimage" tal:attributes="src item['icon']; id item['id']"/>
        </a>
        <blockquote class="startquote">
          <p tal:content="item['description']"> </p>
        </blockquote>
      </div>
    </div>
  </div>
</div>
